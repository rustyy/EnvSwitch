{
  "name": "EnvSwitch",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "EnvSwitch",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "popup.html"
  }
}
